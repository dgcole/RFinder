<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" minHeight="540.0" minWidth="960.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rfinder.Controller.Main">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
      <RowConstraints />
    </rowConstraints>
    <children>
        <MenuBar GridPane.columnIndex="3">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#importStarmap" text="Import Starmap" />
                  <MenuItem mnemonicParsing="false" text="Clear Starmap" />
                        <SeparatorMenuItem />
                        <MenuItem mnemonicParsing="false" onAction="#exit" text="Exit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#about" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS">
        <tabs>
          <Tab text="Tests">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
          <Tab text="Resources">
               <content>
                  <TableView fx:id="resourceTable" prefHeight="200.0" prefWidth="200.0">
                     <columns>
                        <TableColumn fx:id="col1" prefWidth="75.0" resizable="false" text="Resource" />
                        <TableColumn fx:id="col2" prefWidth="75.0" resizable="false" text="Galaxy" />
                        <TableColumn fx:id="col3" prefWidth="75.0" resizable="false" text="Sector" />
                        <TableColumn fx:id="col4" prefWidth="75.0" resizable="false" text="System" />
                        <TableColumn fx:id="col5" prefWidth="75.0" resizable="false" text="Q1" />
                        <TableColumn fx:id="col6" prefWidth="75.0" resizable="false" text="Q2" />
                        <TableColumn fx:id="col7" prefWidth="75.0" resizable="false" text="Q3" />
                        <TableColumn fx:id="col8" prefWidth="75.0" resizable="false" text="A1" />
                        <TableColumn fx:id="col9" prefWidth="75.0" resizable="false" text="A2" />
                        <TableColumn fx:id="col10" prefWidth="75.0" resizable="false" text="A3" />
                     </columns>
                  </TableView>
               </content>
          </Tab>
        </tabs>
      </TabPane>
    </children>
</GridPane>
